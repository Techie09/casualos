        "@casual-simulation/causal-tree-store-mongodb": "^1.3.12",
        "@casual-simulation/causal-trees": "^1.3.12",
        "cassandra-driver": "4.5.1",
        "cli-progress": "3.8.1",
        "colors": "1.4.0",
        "commander": "4.0.1",
        "inquirer": "7.1.0",
        "mongodb": "3.6.2",
        "rxjs": "^6.5.2"
    },
    "devDependencies": {
        "typescript": "^4.1.2",
        "webpack": "^5.10.0",
        "webpack-cli": "^4.2.0",
