{
    "name": "root",
    "private": true,
    "version": "0.3.0",
    "scripts": {
        "start": "node ./src/aux-server/server/dist/main.js",
        "bootstrap": "lerna bootstrap --hoist && npm run bootstrap:dynamodb",
        "bootstrap:dynamodb": "node src/aux-auth/scripts/bootstrap.js",
        "reset:dynamodb": "node src/aux-auth/scripts/bootstrap.js --reset",
        "watch": "npm run clean && npm run fastbuild:portals && concurrently --names \"Libs,CustomPortals,Server,Auth\" \"npm run watch:libs\" \"npm run watch:portals\" \"npm run watch:server\" \"npm run watch:auth\"",
        "watch:server": "lerna exec --no-prefix --scope @casual-simulation/aux-server -- npm run watch",
        "watch:libs": "tsc --build --watch",
        "watch:auth": "lerna exec --no-prefix --scope @casual-simulation/aux-auth -- npm run watch",
        "watch:docs": "cd docs && npm start",
        "watch:portals": "lerna exec --scope @casual-simulation/aux-custom-portals -- npm run watch",
        "rebuild": "npm run clean && npm run build",
        "build": "npm run clean && npm run build:portals && npm run build:libs && npm run build:server && npm run build:auth && npm run build:proxy && npm run build:redirector && npm run build:cli",
        "build:tar:docker": "npm run build && npm run tar:docket",
        "build:docker": "npm run build && npm run package:docker",
        "build:docker:arm32": "npm run build:tar:docker && npm run package:docker:arm32",
        "build:libs": "tsc --build",
        "build:portals": "lerna exec --scope @casual-simulation/aux-custom-portals -- npm run build",
        "build:server": "lerna exec --scope @casual-simulation/aux-server -- npm run build",
        "build:auth": "lerna exec --scope @casual-simulation/aux-auth -- npm run build",
        "build:web": "lerna exec --scope @casual-simulation/aux-server -- npm run build:web",
        "build:proxy": "lerna exec --scope @casual-simulation/aux-proxy -- npm run build",
        "build:redirector": "lerna exec --scope @casual-simulation/aux-redirector -- npm run build",
        "build:cli": "lerna exec --scope @casual-simulation/causal-tree-cli -- npm run build:prod",
        "build:docs": "npm run clean && lerna exec -- npm run build:docs",
        "build:profile": "npm run clean && npm run build:portals && npm run build:libs && lerna exec --scope @casual-simulation/aux-server -- npm run build:profile",
        "build:dev:profile": "npm run clean && npm run build:portals && npm run build:libs && lerna exec --scope @casual-simulation/aux-server -- npm run build:dev:profile",
        "fastbuild:portals": "lerna exec --scope @casual-simulation/aux-custom-portals -- npm run fastbuild",
        "package:docker": "docker build -t casualsimulation/aux .",
        "package:docker:arm32": "docker build -t casualsimulation/aux-arm32 -f Dockerfile.arm32 .",
        "package:docker:proxy": "docker build -t casualsimulation/aux-proxy ./src/aux-proxy",
        "package:docker:redirector": "docker build -t casualsimulation/aux-redirector ./src/aux-redirector",
        "package:config": "node ./script/prerender-web-config.js",
        "test:watch": "npm run clean && jest --watchAll",
        "test": "npm run clean && jest --verbose",
        "test:ci": "npm run clean && jest --no-cache --ci --reporters=default --reporters=jest-junit",
        "test:coverage": "npm test -- --coverage --reporters=default --reporters=jest-html-reporter",
        "clean": "gulp clean",
        "clean:cache": "gulp clean:cache",
        "debug:server:chrome": "node --inspect src/aux-server/server/dist/main.js",
        "tar:docker": "node ./script/make-tar-docker.js",
        "tar:client": "node ./script/make-tar-client.js",
        "view:server:profile": "gulp view:server:profile",
        "view:web:profile": "gulp view:web:profile",
        "bench": "npm run build:libs && lerna exec --scope @casual-simulation/aux-benchmarks -- npm run bench",
        "bench:profile": "npm run build:libs && lerna exec --scope @casual-simulation/aux-benchmarks -- npm run bench:profile",
        "cloc": "cloc src --exclude-dir=node_modules --exclude-ext=js,json,def",
        "import-typescript": "lerna exec --scope @casual-simulation/aux-custom-portals -- npm run import-typescript",
        "esbuild": "node ./script/dev-server.js",
        "vite": "lerna exec --no-prefix --scope @casual-simulation/aux-server -- npm run vite"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "devDependencies": {
        "@babel/core": "^7.14.3",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.14.4",
        "@types/base64-js": "^1.3.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/benchmark": "^2.1.1",
        "@types/bn.js": "^5.1.0",
        "@types/chai": "^4.2.22",
        "@types/commander": "^2.12.2",
        "@types/d3": "^7.1.0",
        "@types/estraverse": "^5.1.1",
        "@types/express": "^4.17.13",
        "@types/mongodb": "^3.5.32",
        "@types/hapi__joi": "^17.1.7",
        "@types/http-proxy": "^1.17.7",
        "@types/jest": "^27.0.2",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/jszip": "^3.4.1",
        "@types/lodash": "^4.14.176",
        "@types/lru-cache": "^5.1.1",
        "@types/pify": "^5.0.1",
        "@types/redis": "^2.8.32",
        "@types/sinon": "^10.0.6",
        "@types/socket.io": "^2.1.2",
        "@types/socket.io-client": "^1.4.32",
        "@types/socket.io-parser": "2.2.1",
        "@types/stripe": "6.32.1",
        "@types/stripe-v3": "^3.1.7",
        "@types/sshpk": "^1.10.7",
        "@types/useragent": "^2.3.1",
        "@types/uuid": "^8.3.1",
        "@types/vue-color": "^2.4.3",
        "@types/ws": "^8.2.0",
        "@types/cors": "^2.8.12",
        "@types/node": "^16.11.6",
        "@types/howler": "^2.2.4",
        "@types/inquirer": "^8.1.3",
        "@types/cli-progress": "^3.9.2",
        "@types/mime": "^2.0.3",
        "@types/estree": "^0.0.50",
        "@types/compression": "^1.7.2",
        "@types/arcgis-js-api": "^4.21.0",
        "@types/bson": "4.0.5",
        "@types/offscreencanvas": "^2019.6.4",
        "braces": "^2.3.2",
        "chai": "^4.2.0",
        "concurrently": "^4.0.1",
        "del": "^3.0.0",
        "gulp": "^4.0.0",
        "handlebars": "^4.4.2",
        "husky": "^4.3.0",
        "jest": "^26.6.3",
        "jest-html-reporter": "^2.5.0",
        "jest-junit": "^6.3.0",
        "jest-raw-loader": "^1.0.1",
        "jest-environment-node": "^26.6.2",
        "js-yaml": "^3.13.1",
        "lerna": "^3.13.4",
        "nodemon": "^1.18.6",
        "precise-commits": "^1.0.2",
        "prettier": "^2.1.1",
        "pretty-quick": "^3.0.2",
        "sinon": "^7.2.4",
        "socket.io-mock": "1.2.3",
        "tar": "4.4.18",
        "ts-jest": "26.4.4",
        "typedoc": "^0.15.0",
        "typescript": "^4.4.4",
        "@types/mock-fs": "4.10.0",
        "mock-fs": "4.10.2",
        "faker": "5.1.0",
        "@types/faker": "5.1.3",
        "@testing-library/vue": "5.6.1",
        "@testing-library/jest-dom": "5.11.9",
        "@casual-simulation/typescript": "^4.2.7",
        "browserslist": ">=4.16.5",
        "vite": "2.6.10",
        "esbuild": "^0.13.8",
        "chalk": "4.1.2",
        "vite-plugin-vue2": "1.7.3",
        "vue-template-compiler": "^2.6.14",
        "wait-on": "6.0.0",
        "chokidar": "3.5.2",
        "rollup-plugin-copy": "3.4.0",
        "vite-plugin-svg-icons": "1.0.5",
        "vite-plugin-pwa": "0.11.3",
        "workbox-precaching": "6.3.0",
        "workbox-core": "6.3.0",
        "workbox-routing": "6.3.0",
        "workbox-strategies": "6.3.0",
        "workbox-expiration": "6.3.0",
        "vite-plugin-html": "2.1.1",
        "source-map": "0.7.3"
    },
    "dependencies": {}
}
